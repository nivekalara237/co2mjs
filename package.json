{
  "name": "co2m.js",
  "version": "1.0.5",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "importSort": {
    ".js, .ts": {
      "stype": "renke"
    }
  },
  "lint-staged": {
    "*.[tj]s": [
      "import-sort-cli --write",
      "prettier --write"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "test": "jest --colors --ci --updateSnapshot --coverage",
    "test:jest": "jest --colors --ci --updateSnapshot --coverage",
    "jest:clear": "jest --clear cache --colors --ci --notity",
    "jest:watch": "jest  --colors --ci --coverage --watch",
    "clean": "rm -rf coverage; rm -rf dist || exit 0",
    "deleteModules": "rm -rf node_modules",
    "dev": "node ./tasks/dev.js",
    "build": "node ./tasks/build.js",
    "lint": "eslint"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "keywords": [
    "JavaScript",
    "Common",
    "JS",
    "Typescript",
    "Utilities",
    "Objects",
    "Math",
    "Boolean",
    "String",
    "Number",
    "Random",
    "utils",
    "Throwable"
  ],
  "homepage": "https://github.com/nivekalara237/co2mjs",
  "bugs": {
    "email": "admin@nivekaa.com",
    "url": "https://github.com/nivekalara237/co2mjs/issues"
  },
  "author": {
    "name": "Kevin L. Kemta",
    "url": "nivekaa.com",
    "email": "admin@nivekaa.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "esbuild": "^0.23.1",
    "esbuild-node-externals": "^1.14.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-text-replace": "^1.3.0",
    "esbuild-plugin-write-file": "^0.0.4",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "import-sort-cli": "^2.4.0",
    "import-sort-style-renke": "^6.0.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.9",
    "node-notifier": "^10.0.1",
    "npm-commands": "^1.2.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "yargs-parser": "^19.0.4"
  }
}
